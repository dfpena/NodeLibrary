{"nodes":[{"nid":140,"uid":"custom","category":"logical","type":"jump2Jupyter","x":1531,"y":498,"fields":{"in":[{"name":"start"}],"out":[{"name":"pass"}]},"python":"@on_start\ndef jump2Jupyter|||(*args,**kwargs):\n    def createNotebook(name,noteboook_string):\n        with open(name, \"wb\") as text_file:\n            text_file.write(str.encode(noteboook_string))\n\n    jupyternotebook = \"\"\"{text}\"\"\"\n    jupyternotebook = jupyternotebook.replace(\"pythonfile123456\",\"{pythonfile123456}\")\n    jupyternotebook = jupyternotebook.replace(\"datasource123456\",\"{datasource123456}\")\n    createNotebook(\"{notebookname}\",jupyternotebook)\n    pickle.dump( kwargs, open( \"{datasource123456}\", \"wb\" ) )\n    os.system('jupyter notebook {notebookname}')\n    \n    \n    return kwargs","python_import":"import pickle\nimport os","python_exec":"jump2Jupyter|||","settings":[{"key":"pythonfile123456","label":"Python file name without extension (ex. MyKoiName):"},{"key":"datasource123456","label":"State save filename (ex. current_state.pkl)"},{"key":"notebookname","label":"Filename to use for the notebook (ex. current.ipynb)"},{"key":"text","label":"Template Jupyter Code","type":"json_text_language"}],"icon":"./static/media/blankNode.38260d5b.svg","data":{"text":"{\n \"cells\": [\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"from pythonfile123456 import * \\\\n\",\n    \"import pickle\\\\n\"\n   ]\n  },\n  {\n   \"cell_type\": \"markdown\",\n   \"metadata\": {},\n   \"source\": [\n    \"This notebook is a template used to import section of a koi to monkey around in jupyter if you need to. This is not meant to be run on its own, but inside stremecoder\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"kwargs = pickle.load( open( \\\\\"datasource123456\\\\\", \\\\\"rb\\\\\" ) )\"\n   ]\n  }