ook (ex. current.ipynb)\"},{\"key\":\"text\",\"label\":\"Template Jupyter Code\",\"type\":\"json_text_language\"}]","stringPins":"{\"in\":[{\"name\":\"start\"}],\"out\":[{\"name\":\"pass\"}]}","togglePins":false,"pintracker":false,"dialogtracker":false,"descriptiontracker":false,"version":"0.0.1","headerStyle":{"background":"#1562ad"}}],"connections":[],"title":"jump2Jupyter","showPopup":false,"current":-1,"count":1,"servers":{},"serverselection":null}